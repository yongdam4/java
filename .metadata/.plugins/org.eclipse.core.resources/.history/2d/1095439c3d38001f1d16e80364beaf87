package d.roof;
import java.util.Scanner;

public class D_while {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		
	while(true) {
		int n = sc.nextInt();
		if(n == 0) {
			break;
		}else {
			System.out.println(n);
		}
	}
		
		/*
		 //사용자가 0을 입력할때 까지 반복해서 숫자를 입력받아 그대로 출력해라
		int i = 0;
		while(true) {
			System.out.println("안녕하세요");
			i++;
		}
		*/
		/*
		 * while 문
		 * [표현법]
		 * 
		 * [초기식]
		 * while(조건식){
		 * 반복할 코드
		 * 증감식;
		 * }
		 */
	}
}
